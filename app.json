{
	"expo": {
		"name": "reclamos-mobile",
		"slug": "reclamos-mobile",
		"privacy": "public",
		"sdkVersion": "35.0.0",
		"platforms": ["ios", "android", "web"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#00a680"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.uade.reclamosmobile",
			"supportsTablet": true
		},
		"android": {
			"package": "com.uade.reclamosmobile",
			"permissions": ["CAMERA_ROLL", "CAMERA", "AUDIO_RECORDING"]
		}
	}
}
